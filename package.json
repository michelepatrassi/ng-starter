{
  "name": "boilerplate",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng run boilerplate:serve:dev",
    "build": "ng build",
    "build:prod": "NODE_ENV=production npm run build -- --prod",
    "test": "ng test",
    "lint": "eslint --color \"src/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "e2e": "ng e2e",
    "dev:ssr": "ng run boilerplate:serve-ssr:dev",
    "serve:ssr": "node dist/boilerplate/server/main.js",
    "build:ssr": "ng build --prod && ng run boilerplate:server:production",
    "prerender": "ng run boilerplate:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.4",
    "@angular/common": "~10.1.4",
    "@angular/compiler": "~10.1.4",
    "@angular/core": "~10.1.4",
    "@angular/forms": "~10.1.4",
    "@angular/platform-browser": "~10.1.4",
    "@angular/platform-browser-dynamic": "~10.1.4",
    "@angular/platform-server": "~10.1.4",
    "@angular/router": "~10.1.4",
    "@angularclass/hmr": "^3.0.0",
    "@garygrossgarten/ngx-tailwind": "^0.4.0",
    "@nguniversal/express-engine": "^10.1.0",
    "express": "^4.15.2",
    "ngx-logger": "^4.1.9",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.4",
    "@angular/cli": "~10.1.4",
    "@angular/compiler-cli": "~10.1.4",
    "@nguniversal/builders": "^10.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "codelyzer": "^6.0.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ngx-build-plus": "latest",
    "postcss-import": "latest",
    "postcss-loader": "latest",
    "postcss-scss": "latest",
    "prettier": "^2.1.2",
    "protractor": "~7.0.0",
    "tailwindcss": "latest",
    "ts-node": "~8.3.0",
    "typescript": "~4.0.2"
  }
}
